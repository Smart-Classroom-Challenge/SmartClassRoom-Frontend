#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/vue-tsc@0.33.9_typescript@4.6.3/node_modules/vue-tsc/bin/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/vue-tsc@0.33.9_typescript@4.6.3/node_modules/vue-tsc/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/vue-tsc@0.33.9_typescript@4.6.3/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules:/mnt/c/Users/super/Documents/node_modules:/mnt/c/Users/super/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/vue-tsc/bin/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/vue-tsc/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/vue-tsc@0.33.9_typescript@4.6.3/node_modules/vue-tsc/bin/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/vue-tsc@0.33.9_typescript@4.6.3/node_modules/vue-tsc/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/vue-tsc@0.33.9_typescript@4.6.3/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules:/mnt/c/Users/super/Documents/node_modules:/mnt/c/Users/super/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/vue-tsc/bin/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/vue-tsc/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vue-tsc/bin/vue-tsc.js" "$@"
else
  exec node  "$basedir/../vue-tsc/bin/vue-tsc.js" "$@"
fi
