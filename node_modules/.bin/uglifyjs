#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/uglify-js@3.15.3/node_modules/uglify-js/bin/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/uglify-js@3.15.3/node_modules/uglify-js/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/uglify-js@3.15.3/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules:/mnt/c/Users/super/Documents/node_modules:/mnt/c/Users/super/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/uglify-js/bin/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/uglify-js/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/uglify-js@3.15.3/node_modules/uglify-js/bin/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/uglify-js@3.15.3/node_modules/uglify-js/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/uglify-js@3.15.3/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules:/mnt/c/Users/super/Documents/node_modules:/mnt/c/Users/super/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/uglify-js/bin/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/uglify-js/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../uglify-js/bin/uglifyjs" "$@"
else
  exec node  "$basedir/../uglify-js/bin/uglifyjs" "$@"
fi
