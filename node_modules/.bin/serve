#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/super/Documents/SmartClassRoom-Frontend2/node_modules/.pnpm/https-localhost@4.7.1/node_modules/https-localhost/node_modules:/mnt/c/Users/super/Documents/SmartClassRoom-Frontend2/node_modules/.pnpm/https-localhost@4.7.1/node_modules:/mnt/c/Users/super/Documents/SmartClassRoom-Frontend2/node_modules/.pnpm/node_modules:/mnt/c/Users/super/Documents/SmartClassRoom-Frontend2/node_modules:/mnt/c/Users/super/Documents/node_modules:/mnt/c/Users/super/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/super/Documents/SmartClassRoom-Frontend2/node_modules/https-localhost/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/super/Documents/SmartClassRoom-Frontend2/node_modules/.pnpm/https-localhost@4.7.1/node_modules/https-localhost/node_modules:/mnt/c/Users/super/Documents/SmartClassRoom-Frontend2/node_modules/.pnpm/https-localhost@4.7.1/node_modules:/mnt/c/Users/super/Documents/SmartClassRoom-Frontend2/node_modules/.pnpm/node_modules:/mnt/c/Users/super/Documents/SmartClassRoom-Frontend2/node_modules:/mnt/c/Users/super/Documents/node_modules:/mnt/c/Users/super/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/super/Documents/SmartClassRoom-Frontend2/node_modules/https-localhost/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../https-localhost/index.js" "$@"
else
  exec node  "$basedir/../https-localhost/index.js" "$@"
fi
