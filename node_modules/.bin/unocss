#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/@unocss+cli@0.30.8/node_modules/@unocss/cli/bin/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/@unocss+cli@0.30.8/node_modules/@unocss/cli/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/@unocss+cli@0.30.8/node_modules/@unocss/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/@unocss+cli@0.30.8/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules:/mnt/c/Users/super/Documents/node_modules:/mnt/c/Users/super/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/@unocss/cli/bin/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/@unocss/cli/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/@unocss/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/@unocss+cli@0.30.8/node_modules/@unocss/cli/bin/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/@unocss+cli@0.30.8/node_modules/@unocss/cli/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/@unocss+cli@0.30.8/node_modules/@unocss/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/@unocss+cli@0.30.8/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules:/mnt/c/Users/super/Documents/node_modules:/mnt/c/Users/super/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/@unocss/cli/bin/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/@unocss/cli/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/@unocss/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@unocss/cli/bin/unocss.cjs" "$@"
else
  exec node  "$basedir/../@unocss/cli/bin/unocss.cjs" "$@"
fi
