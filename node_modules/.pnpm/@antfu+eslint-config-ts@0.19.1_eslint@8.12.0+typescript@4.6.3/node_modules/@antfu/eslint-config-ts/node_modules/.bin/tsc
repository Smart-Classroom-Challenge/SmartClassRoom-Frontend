#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/bin/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/typescript@4.6.3/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules:/mnt/c/Users/super/Documents/node_modules:/mnt/c/Users/super/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/@antfu+eslint-config-ts@0.19.1_eslint@8.12.0+typescript@4.6.3/node_modules/@antfu/eslint-config-ts/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/@antfu+eslint-config-ts@0.19.1_eslint@8.12.0+typescript@4.6.3/node_modules/@antfu/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/@antfu+eslint-config-ts@0.19.1_eslint@8.12.0+typescript@4.6.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/bin/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/typescript@4.6.3/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules:/mnt/c/Users/super/Documents/node_modules:/mnt/c/Users/super/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/@antfu+eslint-config-ts@0.19.1_eslint@8.12.0+typescript@4.6.3/node_modules/@antfu/eslint-config-ts/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/@antfu+eslint-config-ts@0.19.1_eslint@8.12.0+typescript@4.6.3/node_modules/@antfu/node_modules:/mnt/c/Users/super/Documents/new_template/node_modules/.pnpm/@antfu+eslint-config-ts@0.19.1_eslint@8.12.0+typescript@4.6.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typescript@4.6.3/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../../../typescript@4.6.3/node_modules/typescript/bin/tsc" "$@"
fi
